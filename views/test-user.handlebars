<h2>User Profile</h2>
{{#with user}}
<table>
    <tr>
        <th>ID:</th>
        <td>{{_id}}</td>
    </tr>
    <tr>
        <th>Discription:</th>
        <td>{{discription}}</td>
    </tr>
    <tr>
        <th>First Name:</th>
        <td>{{firstName}}</td>
    </tr>
    <tr>
        <th>Last Name:</th>
        <td>{{lastName}}</td>
    </tr>
    <tr>
        <th>Username:</th>
        <td>{{username}}</td>
    </tr>
    <tr>
        <th>Password Hash:</th>
        <td>{{passwordHash}}</td>
    </tr>
    <tr>
        <th>Email:</th>
        <td>{{email}}</td>
    </tr>
    <tr>
        <th>Self:</th>
        <td>{{self}}</td>
    </tr>
    <tr>
        <th>Family:</th>
        <td>
            <ul>
                {{#each family}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
        </td>
    </tr>
    <tr>
        <th>Created:</th>
        <td>{{created}}</td>
    </tr>
</table>
{{/with}}