{{#with user}}
<table>
    <tr>
        <th>ID:</th>
        <td>{{_id}}</td>
    </tr>
    <tr>
        <th>Discription:</th>
        <td>{{discription}}</td>
    </tr>
    <tr>
        <th>First Name:</th>
        <td>{{first_name}}</td>
    </tr>
    <tr>
        <th>Last Name:</th>
        <td>{{last_name}}</td>
    </tr>
    <tr>
        <th>Username:</th>
        <td>{{username}}</td>
    </tr>
    <tr>
        <th>Password Hash:</th>
        <td>{{password_hash}}</td>
    </tr>
    <tr>
        <th>Email:</th>
        <td>{{email}}</td>
    </tr>
    <tr>
        <th>Family Members:</th>
        <td>
            <ul>
                {{!-- {{#each family_members}}
                <li>this</li>
                {{/each}} --}}
            </ul>
        </td>
    </tr>
    <tr>
        <th>Created:</th>
        <td>{{created}}</td>
    </tr>
</table>
{{/with}}